<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="GitHub" value="https://nuget.pkg.github.com/JTTesting/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <JTTesting>
      <add key="Username" value="${{ github.actor }}" />
      <add key="Password" value="${{ secrets.GITHUB_TOKEN }}" />
    </JTTesting>
  </packageSourceCredentials>
</configuration>
